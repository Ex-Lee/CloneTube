.social-login
    if typeof loggedUser !== "number"
        button.social-login--github
            a(href=routes.github)
                span
                    i.fab.fa-github 
                |Continue with Github
        button.social-login--kakao
            a(href=routes.kakao)
                span
                    img(src="../../static/kakaoIcon.png")
                |Continue with Kakao